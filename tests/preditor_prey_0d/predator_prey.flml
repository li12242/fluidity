        <consistent_interpolation/>
      </prognostic>
    </scalar_field>
  </material_phase>
  <ocean_biology>
    <pznd>
      <source_and_sink_algorithm>
        <string_value lines="20" type="python">import fluidity.ocean_biology as biology

p={}
p["alpha"]=0.01
p["beta"]=0.75
p["gamma"]=0.5
p["g"]=1.5
p["k_N"]=0.5
p["k"]=0.5
p["mu_P"]=0.1
p["mu_Z"]=0.2
p["mu_D"]=0.05
p["p_P"]=0.75
p["v"]=1.5

biology.pznd(state, p)</string_value>
      </source_and_sink_algorithm>
      <scalar_field name="PhotosyntheticRadiation" rank="0">
        <prescribed>
          <mesh name="VelocityMesh"/>
          <value name="WholeMesh">
            <constant>
              <real_value rank="0">100.0</real_value>
            </constant>
          </value>
          <output/>
          <stat/>
          <detectors>
            <exclude_from_detectors/>
          </detectors>
        </prescribed>
      </scalar_field>
    </pznd>
  </ocean_biology>
