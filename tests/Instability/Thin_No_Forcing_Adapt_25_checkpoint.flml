<?xml version="1.0" encoding="utf-8" ?>
<fluidity_options>
    <simulation_name>
        <string_value lines="1">Thin_No_Forcing_Adapt_checkpoint</string_value>
    </simulation_name>
    <problem_type>
        <string_value lines="1">oceans</string_value>
    </problem_type>
    <geometry>
        <dimension replaces="NDIM">
            <integer_value rank="0">2</integer_value>
        </dimension>
        <mesh name="CoordinateMesh">
            <from_file file_name="Thin_No_Forcing_Adapt_CoordinateMesh_25_checkpoint">
                <format name="triangle"></format>
                <stat>
                    <include_in_stat></include_in_stat>
                </stat>
            </from_file>
        </mesh>
        <mesh name="VelocityMesh">
            <from_mesh>
                <mesh name="CoordinateMesh"></mesh>
                <stat>
                    <exclude_from_stat></exclude_from_stat>
                </stat>
            </from_mesh>
        </mesh>
        <mesh name="PressureMesh">
            <from_mesh>
                <mesh name="CoordinateMesh"></mesh>
                <stat>
                    <exclude_from_stat></exclude_from_stat>
                </stat>
            </from_mesh>
        </mesh>
        <quadrature>
            <degree replaces="NGI">
                <integer_value rank="0">4</integer_value>
            </degree>
        </quadrature>
        <mesh name="GeostrophicPressureMesh">
            <from_mesh>
                <mesh name="CoordinateMesh"></mesh>
                <mesh_shape>
                    <polynomial_degree>
                        <integer_value rank="0">2</integer_value>
                    </polynomial_degree>
                </mesh_shape>
                <stat>
                    <exclude_from_stat></exclude_from_stat>
                </stat>
            </from_mesh>
        </mesh>
    </geometry>
    <io>
        <dump_format>
            <string_value>vtk</string_value>
        </dump_format>
        <dump_period>
          <constant replaces="TIMDUM">
              <real_value rank="0">3600</real_value>
          </constant>
        </dump_period>
        <output_mesh name="VelocityMesh"></output_mesh>
        <checkpointing>
            <checkpoint_period_in_dumps>
                <integer_value rank="0">5</integer_value>
            </checkpoint_period_in_dumps>
        </checkpointing>
        <stat></stat>
    </io>
    <timestepping>
        <current_time replaces="ACCTIM">
            <real_value rank="0">90000</real_value>
        </current_time>
        <timestep replaces="DT">
            <real_value rank="0">300</real_value>
        </timestep>
        <finish_time replaces="LTIME">
            <real_value rank="0">93600</real_value>
        </finish_time>
    </timestepping>
    <physical_parameters>
        <gravity>
            <magnitude replaces="GRAVTY">
                <real_value rank="0">9.81</real_value>
            </magnitude>
            <vector_field name="GravityDirection" replaces="BSOUX BSOUY BSOUZ" rank="1">
                <prescribed>
                    <mesh name="CoordinateMesh"></mesh>
                    <value name="WholeMesh">
                        <constant>
                            <real_value rank="1" shape="2" dim1="dim">0 -1</real_value>
                        </constant>
                    </value>
                    <output></output>
                    <stat>
                        <include_in_stat></include_in_stat>
                    </stat>
                    <detectors>
                        <exclude_from_detectors></exclude_from_detectors>
                    </detectors>
                </prescribed>
            </vector_field>
        </gravity>
    </physical_parameters>
    <material_phase name="Water">
        <equation_of_state>
            <fluids>
                <linear replaces="EQNSTA=0 or 1">
                    <reference_density replaces="DENINI">
                        <real_value rank="0">1</real_value>
                    </reference_density>
                    <temperature_dependency replaces="BOUSIN, EQNSTA=0">
                        <reference_temperature replaces="TEMINI">
                            <real_value rank="0">0</real_value>
                        </reference_temperature>
                        <thermal_expansion_coefficient replaces="DENGAM">
                            <real_value rank="0">0.0002</real_value>
                        </thermal_expansion_coefficient>
                    </temperature_dependency>
                    <subtract_out_hydrostatic_level replaces="BHOUT"></subtract_out_hydrostatic_level>
                </linear>
            </fluids>
        </equation_of_state>
        <scalar_field name="Pressure" rank="0">
            <prognostic>
                <mesh name="PressureMesh"></mesh>
                <spatial_discretisation>
                    <continuous_galerkin></continuous_galerkin>
                </spatial_discretisation>
                <reference_node replaces="NDPSET">
                    <integer_value rank="0">1</integer_value>
                </reference_node>
                <scheme>
                    <poisson_pressure_solution replaces="POISON">
                        <string_value lines="1">only first timestep</string_value>
                    </poisson_pressure_solution>
                    <use_projection_method replaces="PROJEC"></use_projection_method>
                </scheme>
                <solver>
                    <iterative_method name="cg"></iterative_method>
                    <preconditioner name="sor"></preconditioner>
                    <relative_error>
                        <real_value rank="0">1e-10</real_value>
                    </relative_error>
                    <max_iterations>
                        <integer_value rank="0">1700</integer_value>
                    </max_iterations>
                    <never_ignore_solver_failures></never_ignore_solver_failures>
                    <diagnostics>
                        <monitors></monitors>
                    </diagnostics>
                </solver>
                <initial_condition name="WholeMesh">
                    <from_file file_name="Thin_No_Forcing_Adapt_PressureMesh_25_checkpoint.vtu">
                        <format name="vtu"></format>
                    </from_file>
                </initial_condition>
                <output></output>
                <stat></stat>
                <convergence>
                    <include_in_convergence></include_in_convergence>
                </convergence>
                <detectors>
                    <exclude_from_detectors></exclude_from_detectors>
                </detectors>
                <steady_state>
                    <include_in_steady_state></include_in_steady_state>
                </steady_state>
                <consistent_interpolation></consistent_interpolation>
            </prognostic>
        </scalar_field>
        <scalar_field name="Density" rank="0">
            <diagnostic>
                <algorithm name="Internal" material_phase_support="multiple"></algorithm>
                <mesh name="VelocityMesh"></mesh>
                <output></output>
                <stat></stat>
                <convergence>
                    <include_in_convergence></include_in_convergence>
                </convergence>
                <detectors>
                    <include_in_detectors></include_in_detectors>
                </detectors>
                <steady_state>
                    <include_in_steady_state></include_in_steady_state>
                </steady_state>
            </diagnostic>
        </scalar_field>
        <vector_field name="Velocity" rank="1">
            <prognostic>
                <mesh name="VelocityMesh"></mesh>
                <equation name="Boussinesq"></equation>
                <spatial_discretisation>
                    <continuous_galerkin>
                        <stabilisation>
                            <no_stabilisation></no_stabilisation>
                        </stabilisation>
                        <mass_terms>
                            <lump_mass_matrix></lump_mass_matrix>
                        </mass_terms>
                        <advection_terms></advection_terms>
                        <stress_terms>
                            <tensor_form></tensor_form>
                        </stress_terms>
                    </continuous_galerkin>
                    <conservative_advection replaces="BETA">
                        <real_value rank="0">0</real_value>
                    </conservative_advection>
                </spatial_discretisation>
                <temporal_discretisation>
                    <theta replaces="THETA">
                        <real_value rank="0">0.5</real_value>
                    </theta>
                    <relaxation replaces="ITHETA">
                        <real_value rank="0">0.5</real_value>
                    </relaxation>
                </temporal_discretisation>
                <solver>
                    <iterative_method name="gmres">
                        <restart>
                            <integer_value rank="0">30</integer_value>
                        </restart>
                    </iterative_method>
                    <preconditioner name="sor"></preconditioner>
                    <relative_error>
                        <real_value rank="0">1e-10</real_value>
                    </relative_error>
                    <max_iterations>
                        <integer_value rank="0">1000</integer_value>
                    </max_iterations>
                    <never_ignore_solver_failures></never_ignore_solver_failures>
                    <diagnostics>
                        <monitors></monitors>
                    </diagnostics>
                </solver>
                <initial_condition name="WholeMesh">
                    <from_file file_name="Thin_No_Forcing_Adapt_VelocityMesh_25_checkpoint.vtu">
                        <format name="vtu"></format>
                    </from_file>
                </initial_condition>
                <boundary_conditions name="LeftRight" replaces="boundary, TMPER1 TMPER2 TMPERI">
                    <surface_ids>
                        <integer_value rank="1" shape="2">5 6</integer_value>
                    </surface_ids>
                    <type name="dirichlet">
                        <align_bc_with_cartesian>
                            <x_component>
                                <constant>
                                    <real_value rank="0">0</real_value>
                                </constant>
                            </x_component>
                        </align_bc_with_cartesian>
                    </type>
                </boundary_conditions>
                <boundary_conditions name="TopBottom" replaces="boundary, TMPER1 TMPER2 TMPERI">
                    <surface_ids>
                        <integer_value rank="1" shape="2">7 8</integer_value>
                    </surface_ids>
                    <type name="dirichlet">
                        <align_bc_with_cartesian>
                            <y_component>
                                <constant>
                                    <real_value rank="0">0</real_value>
                                </constant>
                            </y_component>
                        </align_bc_with_cartesian>
                    </type>
                </boundary_conditions>
                <tensor_field name="Viscosity" replaces="MUPTXX MUPTYY MUPTZZ MUPTYZ MUPTXZ MUPTXY RMUPXX RMUPYY RMUPZZ RMUPYZ RMUPXZ RMUPXY CONMU ALLMU TWOMU ONEMU" rank="2">
                    <prescribed>
                        <value name="WholeMesh">
                            <isotropic>
                                <constant>
                                    <real_value rank="0">0.1</real_value>
                                </constant>
                            </isotropic>
                        </value>
                        <output></output>
                    </prescribed>
                </tensor_field>
                <output></output>
                <stat>
                    <include_in_stat></include_in_stat>
                    <previous_time_step>
                        <exclude_from_stat></exclude_from_stat>
                    </previous_time_step>
                    <nonlinear_field>
                        <exclude_from_stat></exclude_from_stat>
                    </nonlinear_field>
                </stat>
                <convergence>
                    <include_in_convergence></include_in_convergence>
                </convergence>
                <detectors>
                    <include_in_detectors></include_in_detectors>
                </detectors>
                <steady_state>
                    <include_in_steady_state></include_in_steady_state>
                </steady_state>
                <adaptivity_options>
                    <absolute_measure>
                        <vector_field name="InterpolationErrorBound" replaces="ADWEIU ADWEIV ADWEIW" rank="1">
                            <prescribed>
                                <value name="WholeMesh">
                                    <constant>
                                        <real_value rank="1" shape="2" dim1="dim">0.002 0.002</real_value>
                                    </constant>
                                </value>
                                <output></output>
                                <stat>
                                    <include_in_stat></include_in_stat>
                                </stat>
                                <detectors>
                                    <exclude_from_detectors></exclude_from_detectors>
                                </detectors>
                            </prescribed>
                        </vector_field>
                    </absolute_measure>
                </adaptivity_options>
                <consistent_interpolation></consistent_interpolation>
            </prognostic>
        </vector_field>
        <scalar_field name="GeostrophicPressure" replaces="IDENT = -2003" rank="0">
            <prognostic>
                <mesh name="GeostrophicPressureMesh"></mesh>
                <spatial_discretisation>
                    <geostrophic_pressure_option replaces="GEOBAL">
                        <string_value>include_buoyancy</string_value>
                    </geostrophic_pressure_option>
                </spatial_discretisation>
                <solver>
                    <iterative_method name="cg"></iterative_method>
                    <preconditioner name="sor"></preconditioner>
                    <relative_error>
                        <real_value rank="0">1e-10</real_value>
                    </relative_error>
                    <max_iterations>
                        <integer_value rank="0">3000</integer_value>
                    </max_iterations>
                    <never_ignore_solver_failures></never_ignore_solver_failures>
                    <diagnostics>
                        <monitors></monitors>
                    </diagnostics>
                </solver>
                <output></output>
                <stat></stat>
                <convergence>
                    <include_in_convergence></include_in_convergence>
                </convergence>
                <detectors>
                    <include_in_detectors></include_in_detectors>
                </detectors>
                <steady_state>
                    <include_in_steady_state></include_in_steady_state>
                </steady_state>
                <consistent_interpolation></consistent_interpolation>
                <reference_node name="node_1">
                    <integer_value rank="0">1</integer_value>
                </reference_node>
            </prognostic>
        </scalar_field>
        <scalar_field name="CFLNumber" replaces="IDENT = -601" rank="0">
            <diagnostic>
                <algorithm name="Internal" material_phase_support="multiple"></algorithm>
                <mesh name="VelocityMesh"></mesh>
                <output></output>
                <stat></stat>
                <convergence>
                    <include_in_convergence></include_in_convergence>
                </convergence>
                <detectors>
                    <include_in_detectors></include_in_detectors>
                </detectors>
                <steady_state>
                    <include_in_steady_state></include_in_steady_state>
                </steady_state>
            </diagnostic>
        </scalar_field>
        <scalar_field name="InitialTemperature" rank="0">
            <prescribed>
                <mesh name="VelocityMesh"></mesh>
                <value name="WholeMesh">
                    <python>
                        <string_value lines="20" type="python">def val(X, t):&#x0A;   return (0.025-(X[1]/40000.0))</string_value>
                    </python>
                </value>
                <output></output>
                <stat></stat>
                <detectors>
                    <exclude_from_detectors></exclude_from_detectors>
                </detectors>
            </prescribed>
        </scalar_field>
        <scalar_field name="Temperature" replaces="IDENT = -1" rank="0">
            <prognostic>
                <mesh name="VelocityMesh"></mesh>
                <equation name="AdvectionDiffusion"></equation>
                <spatial_discretisation>
                    <control_volumes>
                        <face_value name="FirstOrderUpwind" replaces="MOD(INT(ABS(NDISOT)/1),10) = 0,1"></face_value>
                        <diffusion_scheme name="BassiRebay"></diffusion_scheme>
                    </control_volumes>
                    <conservative_advection replaces="TBETA, DEFALT (TBETA = 0.0)">
                        <real_value rank="0">1</real_value>
                    </conservative_advection>
                </spatial_discretisation>
                <temporal_discretisation>
                    <theta replaces="TTHETA, DEFALT (TTHETA = 0.5)">
                        <real_value rank="0">0.5</real_value>
                    </theta>
                </temporal_discretisation>
                <solver>
                    <iterative_method name="gmres">
                        <restart>
                            <integer_value rank="0">30</integer_value>
                        </restart>
                    </iterative_method>
                    <preconditioner name="sor"></preconditioner>
                    <relative_error>
                        <real_value rank="0">1e-10</real_value>
                    </relative_error>
                    <max_iterations>
                        <integer_value rank="0">1000</integer_value>
                    </max_iterations>
                    <never_ignore_solver_failures></never_ignore_solver_failures>
                    <diagnostics>
                        <monitors></monitors>
                    </diagnostics>
                </solver>
                <initial_condition name="WholeMesh">
                    <from_file file_name="Thin_No_Forcing_Adapt_VelocityMesh_25_checkpoint.vtu">
                        <format name="vtu"></format>
                    </from_file>
                </initial_condition>
                <tensor_field name="Diffusivity" replaces="TMUXX TMUYY TMUZZ TMUYZ TMUXZ TMUXY TALLMU TCONMU" rank="2">
                    <prescribed>
                        <value name="WholeMesh">
                            <isotropic>
                                <constant>
                                    <real_value rank="0">0.1</real_value>
                                </constant>
                            </isotropic>
                        </value>
                        <output></output>
                    </prescribed>
                </tensor_field>
                <output></output>
                <stat>
                    <surface_integral name="HeatFlux1" type="gradient_normal">
                        <surface_ids>
                            <integer_value rank="1" shape="1">5</integer_value>
                        </surface_ids>
                    </surface_integral>
                    <surface_integral name="HeatFlux2" type="gradient_normal">
                        <surface_ids>
                            <integer_value rank="1" shape="1">6</integer_value>
                        </surface_ids>
                    </surface_integral>
                    <surface_integral name="HeatFlux3" type="gradient_normal">
                        <surface_ids>
                            <integer_value rank="1" shape="1">7</integer_value>
                        </surface_ids>
                    </surface_integral>
                    <surface_integral name="HeatFlux4" type="gradient_normal">
                        <surface_ids>
                            <integer_value rank="1" shape="1">8</integer_value>
                        </surface_ids>
                    </surface_integral>
                    <surface_integral name="Hf_Total" type="gradient_normal"></surface_integral>
                </stat>
                <convergence>
                    <include_in_convergence></include_in_convergence>
                </convergence>
                <detectors>
                    <include_in_detectors></include_in_detectors>
                </detectors>
                <steady_state>
                    <include_in_steady_state></include_in_steady_state>
                </steady_state>
                <adaptivity_options>
                    <absolute_measure replaces="ADOPTT = 0">
                        <scalar_field name="InterpolationErrorBound" replaces="ADWEIT" rank="0">
                            <prescribed>
                                <value name="WholeMesh">
                                    <constant>
                                        <real_value rank="0">0.004</real_value>
                                    </constant>
                                </value>
                                <output></output>
                                <stat></stat>
                                <detectors>
                                    <exclude_from_detectors></exclude_from_detectors>
                                </detectors>
                            </prescribed>
                        </scalar_field>
                    </absolute_measure>
                </adaptivity_options>
                <galerkin_projection>
                    <continuous>
                        <solver>
                            <iterative_method name="cg"></iterative_method>
                            <preconditioner name="sor"></preconditioner>
                            <relative_error>
                                <real_value rank="0">1e-09</real_value>
                            </relative_error>
                            <max_iterations>
                                <integer_value rank="0">1000</integer_value>
                            </max_iterations>
                            <never_ignore_solver_failures></never_ignore_solver_failures>
                            <diagnostics>
                                <monitors></monitors>
                            </diagnostics>
                        </solver>
                    </continuous>
                </galerkin_projection>
            </prognostic>
        </scalar_field>
        <scalar_field name="VolumeTemperatureSource" rank="0">
            <prescribed>
                <mesh name="VelocityMesh"></mesh>
                <value name="WholeMesh">
                    <python>
                        <string_value lines="20" type="python">def val(X,t):&#x0A;&#x09;import math&#x09;&#x0A;&#x09;if X[1]&gt;1800.0:&#x0A;&#x09;&#x09;F_xy = 1.0&#x0A;&#x09;else:&#x0A;&#x09;&#x09;F_xy = 0.0&#x0A;&#x0A;&#x09;return -1.0*10**-6*F_xy</string_value>
                    </python>
                </value>
                <output></output>
                <stat></stat>
                <detectors>
                    <exclude_from_detectors></exclude_from_detectors>
                </detectors>
            </prescribed>
        </scalar_field>
    </material_phase>
    <mesh_adaptivity>
        <hr_adaptivity replaces="ADMESH">
            <period replaces="TIMMES">
                <real_value rank="0">7200</real_value>
            </period>
            <maximum_number_of_nodes replaces="MXNODS">
                <integer_value rank="0">150000</integer_value>
            </maximum_number_of_nodes>
            <enable_gradation></enable_gradation>
            <tensor_field name="MinimumEdgeLengths">
                <anisotropic_symmetric>
                    <constant>
                        <real_value rank="2" shape="2 2" symmetric="true" dim2="dim" dim1="dim">100 0 0 5</real_value>
                    </constant>
                </anisotropic_symmetric>
            </tensor_field>
            <tensor_field name="MaximumEdgeLengths">
                <anisotropic_symmetric>
                    <constant>
                        <real_value rank="2" shape="2 2" symmetric="true" dim2="dim" dim1="dim">1000 0 0 100</real_value>
                    </constant>
                </anisotropic_symmetric>
            </tensor_field>
        </hr_adaptivity>
    </mesh_adaptivity>
</fluidity_options>
