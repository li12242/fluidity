<?xml version='1.0' encoding='utf-8'?>
<fluidity_options>
  <simulation_name>
    <string_value lines="1">parallel_region_ids</string_value>
  </simulation_name>
  <problem_type>
    <string_value lines="1">fluids</string_value>
  </problem_type>
  <geometry>
    <dimension>
      <integer_value rank="0">3</integer_value>
    </dimension>
    <mesh name="CoordinateMesh">
      <from_file file_name="cube-unstructured">
        <format name="triangle"/>
        <stat>
          <include_in_stat/>
        </stat>
      </from_file>
    </mesh>
    <mesh name="VelocityMesh">
      <from_mesh>
        <mesh name="CoordinateMesh"/>
        <mesh_shape>
          <polynomial_degree>
            <integer_value rank="0">0</integer_value>
          </polynomial_degree>
        </mesh_shape>
        <mesh_continuity>
          <string_value>discontinuous</string_value>
        </mesh_continuity>
        <stat>
          <exclude_from_stat/>
        </stat>
      </from_mesh>
    </mesh>
    <quadrature>
      <degree>
        <integer_value rank="0">4</integer_value>
      </degree>
    </quadrature>
  </geometry>
  <io>
    <dump_format>
      <string_value>vtk</string_value>
    </dump_format>
    <dump_period>
      <constant>
        <real_value rank="0">inf</real_value>
      </constant>
    </dump_period>
    <output_mesh name="VelocityMesh"/>
    <stat>
      <output_at_start/>
      <output_before_adapts/>
      <output_after_adapts/>
    </stat>
  </io>
  <timestepping>
    <current_time>
      <real_value rank="0">0.0</real_value>
    </current_time>
    <timestep>
      <real_value rank="0">1.0</real_value>
    </timestep>
    <finish_time>
      <real_value rank="0">2.0</real_value>
    </finish_time>
  </timestepping>
  <material_phase name="Fluid">
    <vector_field name="Velocity" rank="1">
      <prescribed>
        <mesh name="VelocityMesh"/>
        <value name="WholeMesh">
          <python>
            <string_value lines="20" type="python">def val(x, t):
        return x</string_value>
          </python>
        </value>
        <output/>
        <stat>
          <include_in_stat/>
        </stat>
        <detectors>
          <exclude_from_detectors/>
        </detectors>
        <consistent_interpolation/>
        <do_not_recalculate/>
      </prescribed>
    </vector_field>
    <scalar_field name="RegionIDs" rank="0">
      <prescribed>
        <mesh name="VelocityMesh"/>
        <value name="25">
          <region_ids>
            <integer_value shape="1" rank="1">25</integer_value>
          </region_ids>
          <constant>
            <real_value rank="0">25</real_value>
          </constant>
        </value>
        <value name="26">
          <region_ids>
            <integer_value shape="1" rank="1">26</integer_value>
          </region_ids>
          <constant>
            <real_value rank="0">26</real_value>
          </constant>
        </value>
        <value name="27">
          <region_ids>
            <integer_value shape="1" rank="1">27</integer_value>
          </region_ids>
          <constant>
            <real_value rank="0">27</real_value>
          </constant>
        </value>
        <value name="28">
          <region_ids>
            <integer_value shape="1" rank="1">28</integer_value>
          </region_ids>
          <constant>
            <real_value rank="0">28</real_value>
          </constant>
        </value>
        <output/>
        <stat/>
        <detectors>
          <exclude_from_detectors/>
        </detectors>
      </prescribed>
    </scalar_field>
    <scalar_field name="ElementOwner" rank="0">
      <diagnostic>
        <algorithm name="element_ownership" material_phase_support="single"/>
        <mesh name="VelocityMesh"/>
        <output/>
        <stat/>
        <convergence>
          <include_in_convergence/>
        </convergence>
        <detectors>
          <include_in_detectors/>
        </detectors>
        <steady_state>
          <include_in_steady_state/>
        </steady_state>
      </diagnostic>
    </scalar_field>
  </material_phase>
  <mesh_adaptivity>
    <hr_adaptivity>
      <period>
        <real_value rank="0">1</real_value>
      </period>
      <maximum_number_of_nodes>
        <integer_value rank="0">100000</integer_value>
      </maximum_number_of_nodes>
      <disable_gradation/>
      <tensor_field name="MinimumEdgeLengths">
        <anisotropic_symmetric>
          <constant>
            <real_value symmetric="true" dim2="dim" shape="3 3" dim1="dim" rank="2">1.0e-6 0.0 0.0 0.0 1.0e-6 0.0 0.0 0.0 1.0e-6</real_value>
          </constant>
        </anisotropic_symmetric>
      </tensor_field>
      <tensor_field name="MaximumEdgeLengths">
        <anisotropic_symmetric>
          <constant>
            <real_value symmetric="true" dim2="dim" shape="3 3" dim1="dim" rank="2">1.0e6 0.0 0.0 0.0 1.0e6 0.0 0.0 0.0 1.0e6</real_value>
          </constant>
        </anisotropic_symmetric>
      </tensor_field>
      <preserve_mesh_regions/>
      <adaptivity_library>
        <libadaptivity>
          <sweeps>
            <integer_value rank="0">0</integer_value>
          </sweeps>
        </libadaptivity>
      </adaptivity_library>
    </hr_adaptivity>
  </mesh_adaptivity>
</fluidity_options>
