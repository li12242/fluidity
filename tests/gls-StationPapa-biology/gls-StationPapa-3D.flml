              <python>
                <string_value lines="20" type="python">global interp_wind_x,interp_wind_y
from scipy.interpolate import interp1d
from numpy import fromfile, arange, array, sqrt

pp=fromfile("forcing.dat", sep=" ").reshape(143,5)
xstress=array(pp[:,2])
ystress=array(pp[:,3])
times=array([5*24*60*60*i for i in arange(143)])

interp_wind_x=interp1d(times,xstress,bounds_error=False,fill_value=xstress[-1])
interp_wind_y=interp1d(times,ystress,bounds_error=False,fill_value=ystress[-1])

def val(X,t):
    T=t%times[-1]
    return [float(interp_wind_x(T))/1040.0,  float(interp_wind_y(T))/1040.0]</string_value>
                <comment>rho = 1040.</comment>
              </python>
            </wind_stress>
