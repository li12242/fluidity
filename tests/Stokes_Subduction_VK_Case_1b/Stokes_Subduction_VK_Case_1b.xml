<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE testproblem SYSTEM "regressiontest.dtd">
<testproblem>
  <name>Stokes-Subduction-VK-Case-1b</name>
  <owner userid="rhodrid"/>

  <tags>flml</tags>

  <problem_definition length="medium" nprocs="1">
    <command_line>fluidity -v2 -l Stokes-Subduction-VK-Case-1b.flml</command_line>
  </problem_definition>

<variables>  

<variable name="T60" language="python">
from fluidity_tools import stat_parser as stat
T60 = stat("VK_Case_1b.detectors")["Fluid"]["Temperature"]["Temperature6060"][-1]
</variable>

</variables>

<pass_tests>

<test name="Temperature at 60/60: 620.lt. T .lt. 630" language="python">
assert(abs(T60-625.) &lt; 5)
</test>

</pass_tests>                                                                                                                                                                                                

<warn_tests>
</warn_tests>

</testproblem>





