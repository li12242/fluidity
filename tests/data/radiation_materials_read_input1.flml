<?xml version='1.0' encoding='utf-8'?>
<fluidity_options>
  <simulation_name>
    <string_value lines="1">test_radiation_materials_read_input1</string_value>
  </simulation_name>
  <problem_type>
    <string_value lines="1">fluids</string_value>
  </problem_type>
  <geometry>
    <dimension>
      <integer_value rank="0">3</integer_value>
    </dimension>
    <mesh name="CoordinateMesh">
      <from_file file_name="dummy">
        <format name="triangle"/>
        <stat>
          <include_in_stat/>
        </stat>
      </from_file>
    </mesh>
    <mesh name="ParticleMesh">
      <from_mesh>
        <mesh name="CoordinateMesh"/>
        <stat>
          <exclude_from_stat/>
        </stat>
      </from_mesh>
    </mesh>
    <mesh name="ParticleMaterialMesh">
      <from_mesh>
        <mesh name="CoordinateMesh"/>
        <mesh_shape>
          <polynomial_degree>
            <integer_value rank="0">0</integer_value>
          </polynomial_degree>
        </mesh_shape>
        <mesh_continuity>
          <string_value>discontinuous</string_value>
        </mesh_continuity>
        <stat>
          <exclude_from_stat/>
        </stat>
      </from_mesh>
    </mesh>
    <quadrature>
      <degree>
        <integer_value rank="0">4</integer_value>
      </degree>
    </quadrature>
  </geometry>
  <io>
    <dump_format>
      <string_value>vtk</string_value>
    </dump_format>
    <dump_period>
      <constant>
        <real_value rank="0">1.0</real_value>
      </constant>
    </dump_period>
    <output_mesh name="VelocityMesh"/>
    <stat/>
  </io>
  <timestepping>
    <current_time>
      <real_value rank="0">0.0</real_value>
    </current_time>
    <timestep>
      <real_value rank="0">1.0</real_value>
    </timestep>
    <finish_time>
      <real_value rank="0">1.0</real_value>
    </finish_time>
  </timestepping>
  <material_phase name="dummy">
    <vector_field name="Velocity" rank="1">
      <prescribed>
        <mesh name="VelocityMesh"/>
        <value name="WholeMesh">
          <constant>
            <real_value shape="3" dim1="dim" rank="1">0.0 0.0 0.0</real_value>
          </constant>
        </value>
        <output/>
        <stat>
          <include_in_stat/>
        </stat>
        <detectors>
          <exclude_from_detectors/>
        </detectors>
      </prescribed>
    </vector_field>
  </material_phase>
  <radiation>
    <particle_type name="neutron">
      <eigenvalue_run>
        <power_iteration>
          <maximum>
            <integer_value rank="0">100</integer_value>
          </maximum>
          <flux_tolerance>
            <real_value rank="0">1.0e-8</real_value>
          </flux_tolerance>
          <keff_tolerance>
            <real_value rank="0">1.0e-8</real_value>
          </keff_tolerance>
        </power_iteration>
        <flux_normalisation>
          <total_power>
            <real_value rank="0">1.0</real_value>
          </total_power>
        </flux_normalisation>
      </eigenvalue_run>
      <method name="MultiGroupDiffusion">
        <number_of_energy_groups>
          <integer_value rank="0">2</integer_value>
        </number_of_energy_groups>
        <neutral_particle_flux_group_set_field name="all">
          <group_all/>
          <mesh name="ParticleMaterialMesh"/>
          <scalar_field name="NeutralParticleFlux">
            <prognostic>
              <mesh name="ParticleMesh"/>
              <spatial_discretisation>
                <continuous_galerkin/>
              </spatial_discretisation>
              <solver>
                <iterative_method name="cg"/>
                <preconditioner name="sor"/>
                <relative_error>
                  <real_value rank="0">1.0e-08</real_value>
                </relative_error>
                <max_iterations>
                  <integer_value rank="0">100</integer_value>
                </max_iterations>
                <never_ignore_solver_failures/>
                <diagnostics>
                  <monitors/>
                </diagnostics>
              </solver>
              <initial_condition name="WholeMesh">
                <constant>
                  <real_value rank="0">1.0</real_value>
                </constant>
              </initial_condition>
              <output/>
              <stat/>
              <convergence>
                <include_in_convergence/>
              </convergence>
              <detectors>
                <include_in_detectors/>
              </detectors>
              <steady_state>
                <include_in_steady_state/>
              </steady_state>
              <consistent_interpolation/>
            </prognostic>
          </scalar_field>
        </neutral_particle_flux_group_set_field>
      </method>
      <radiation_material_data_set_from_file file_name="data/radiation_materials_read_input1.radmats">
        <number_of_scatter_moments>
          <integer_value rank="0">2</integer_value>
        </number_of_scatter_moments>
        <format_radmats>
          <maximum_record_length>
            <integer_value rank="0">132</integer_value>
          </maximum_record_length>
        </format_radmats>
        <physical_material name="fuel">
          <interpolation_dimension name="one">
            <interpolation_attribute_prescribed>
              <real_value rank="0">20.0</real_value>
            </interpolation_attribute_prescribed>
            <interpolation_values>
              <real_value shape="10" rank="1">10.0 20.0 30.0 40.0 50.0 60.0 70.0 80.0 90.0 100.0</real_value>
            </interpolation_values>
          </interpolation_dimension>
        </physical_material>
      </radiation_material_data_set_from_file>
      <delayed_neutron_precursor>
        <number_delayed_neutron_precursor_groups>
          <integer_value rank="0">6</integer_value>
        </number_delayed_neutron_precursor_groups>
        <read_delayed_lambda_spectrum_from_data_set file_name="data/radiation_materials_read_input1.radmats"/>
      </delayed_neutron_precursor>
    </particle_type>
  </radiation>
</fluidity_options>
