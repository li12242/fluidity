SHELL = @SHELL@

FC      = @FC@
FCFLAGS = -I../include @MOD_FLAG@../include @FCFLAGS@ -I/usr/local/include -I./

CC      = @CC@
CFLAGS  = @CFLAGS@ -I../include -I/usr/local/include

CXX     = @CXX@
CXXFLAGS= @CXXFLAGS@ -I../include -I/usr/local/include

MAKE    = @MAKE@
AR  = @AR@
ARFLAGS = @ARFLAGS@

LIB = ../lib/lib@FLUIDITY@.a

OBJS = Gent_McWilliams.o Equation_of_State.o \
Gls_vertical_turbulence_model.o \
k_epsilon.o \
iceshelf_meltrate_surf_normal.o

.SUFFIXES: .F90 .c .o .a

.F90.o:
	@echo "        FC $<"
	$(FC) $(FCFLAGS) $(GENFLAGS) -c $< 

$(LIB): $(OBJS)
	@mkdir -p ../lib
	@$(AR) $(ARFLAGS) $(LIB) $(OBJS)
default: $(LIB)

clean:
	rm -f *.o *.d *.mod

