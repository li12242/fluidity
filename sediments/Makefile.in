SHELL = @SHELL@

FC      = @FC@
FCFLAGS = -I../include @MOD_FLAG@../include @FCFLAGS@ -I/usr/local/include -I./

CC      = @CC@
CFLAGS  = @CFLAGS@ -I../include -I/usr/local/include

CXX     = @CXX@
CXXFLAGS= @CXXFLAGS@ -I../include -I/usr/local/include

MAKE    = @MAKE@
AR  = @AR@
ARFLAGS = @ARFLAGS@

LIB = ../lib/lib@FLUIDITY@.a

OBJS = Sediment.o Sediment_Diagnostics.o \

.SUFFIXES: .f90 .F90 .c .o .a

.f90.o:
	@echo "        FC $<"
	$(FC) $(FCFLAGS) -c $< || (rm $(LIB) && false)
.F90.o:
	@echo "        FC $<"
	$(FC) $(FCFLAGS) $(GENFLAGS) -c $< || (rm $(LIB) && false)

default: $(OBJS)
	$(AR) $(ARFLAGS) $(LIB) $(OBJS)

clean:
	rm -f *.o *.d *.mod

